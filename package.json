{
  "name": "oxygen",
  "version": "1.0.0",
  "description": "web app for oxygen supplier",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon -q -r dotenv/config src/index.js",
    "seed": "node -r dotenv/config seed/index.js",
    "test": "jest --setupFiles dotenv/config --watchAll --no-cache",
    "test:ci": "jest"
  },
  "_moduleAliases": {
    "@app": "src/app.js",
    "@models": "src/models",
    "@shared": "src/shared"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ],
    "moduleNameMapper": {
      "@app": "<rootDir>/src/app.js",
      "@models/(.*)": "<rootDir>/src/models/$1",
      "@shared/(.*)": "<rootDir>/src/shared/$1"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phyokyawdev/oxygen.git"
  },
  "keywords": [
    "web",
    "app",
    "oxygen",
    "supplier"
  ],
  "author": "Phyo Kyaw",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/phyokyawdev/oxygen/issues"
  },
  "homepage": "https://github.com/phyokyawdev/oxygen#readme",
  "dependencies": {
    "cookie-session": "^1.4.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "joi": "^17.4.1",
    "jsonwebtoken": "^8.5.1",
    "logform": "^2.2.0",
    "module-alias": "^2.2.2",
    "mongoose": "^5.13.3",
    "myanmar-phonenumber": "^0.3.2",
    "passport": "^0.4.1",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "csv-parser": "^3.0.0",
    "debug": "^2.6.9",
    "dotenv": "^10.0.0",
    "jest": "^27.0.6",
    "mongodb-memory-server": "^7.3.6",
    "nodemon": "^2.0.12",
    "supertest": "^6.1.4"
  }
}
